[ignore]
.*/node_modules/stylelint/**/*
.*/vendor/bundle/**/*
.*/tmp/*
.*/dist/*
.*/webpack.config.js
.*/node_modules/@mapbox/jsonlint-lines-primitives/test/*

[include]

[libs]
flow-typed
node_modules/mapbox-gl/flow-typed

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
